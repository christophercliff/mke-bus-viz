{
  "name": "mke-bus-viz",
  "version": "0.0.0",
  "description": "",
  "main": "./lib/",
  "scripts": {
    "build": "npm run jshint && node ./build",
    "jshint": "./node_modules/.bin/jshint ./lib/ ./src/js/ ./build.js ./test/",
    "deploy": "npm run jshint && node ./build && cd ./build && git init . && git add . && git commit -m 'Deploy'; git push 'git@github.com:christophercliff/mke-bus-viz.git' gh-pages --force && rm -rf .git",
    "dev": "npm run jshint && node ./build dev",
    "test": "npm run jshint && ./node_modules/.bin/mocha --reporter spec --require should --grep ''"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/christophercliff/mke-bus-viz.git"
  },
  "dependencies": {
    "handlebars": "^1.3.0",
    "lodash-node": "^2.4.1",
    "mke-bus": "^0.1.0"
  },
  "devDependencies": {
    "bluebird": "^2.2.2",
    "connect": "^3.0.2",
    "jshint": "^2.5.2",
    "json-loader": "^0.5.1",
    "metalsmith": "^0.9.0",
    "metalsmith-fingerprint": "0.0.3",
    "metalsmith-ignore": "^0.1.2",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-simplewatch": "0.0.1",
    "metalsmith-templates": "^0.5.2",
    "metalsmith-webpack": "^0.1.1",
    "mocha": "^1.21.3",
    "polyline": "0.0.3",
    "serve-static": "^1.3.2",
    "should": "^4.0.4",
    "webpack": "^1.3.1"
  },
  "jshintConfig": {
    "eqeqeq": true,
    "immed": true,
    "latedef": "nofunc",
    "newcap": true,
    "quotmark": "single",
    "trailing": true,
    "unused": true,
    "asi": true,
    "boss": true,
    "expr": true,
    "laxbreak": true,
    "laxcomma": true,
    "scripturl": true,
    "sub": true,
    "loopfunc": true
  }
}

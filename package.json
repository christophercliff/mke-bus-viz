{
  "name": "mke-bus-viz",
  "version": "0.0.0",
  "description": "",
  "main": "./lib/",
  "scripts": {
    "build": "npm run jshint && npm run test && node ./build",
    "jshint": "npm run jshint-dev && ./node_modules/.bin/jshint ./src/js/",
    "jshint-dev": "./node_modules/.bin/jshint ./lib/ ./build.js ./test/",
    "deploy": "npm run build && cd ./build && git init . && git add . && git commit -m 'Deploy'; git push 'git@github.com:christophercliff/mke-bus-viz.git' gh-pages --force && rm -rf .git",
    "dev": "npm run jshint && node ./build dev",
    "start": "npm run dev",
    "test": "npm run jshint && ./node_modules/.bin/mocha --reporter spec --require should --grep ''",
    "write": "npm run jshint && node ./lib/write-data"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/christophercliff/mke-bus-viz.git"
  },
  "dependencies": {
    "async": "^0.9.0",
    "d3": "^3.4.11",
    "handlebars": "^1.3.0",
    "mke-bus": "^0.1.0",
    "underscore": "^1.6.0"
  },
  "devDependencies": {
    "bluebird": "^2.2.2",
    "connect": "^3.0.2",
    "geojsonhint": "^0.3.3",
    "handlebars-loader": "^0.1.7",
    "jshint": "^2.5.2",
    "json-loader": "^0.5.1",
    "metalsmith": "^0.9.0",
    "metalsmith-fingerprint": "0.0.3",
    "metalsmith-ignore": "^0.1.2",
    "metalsmith-less": "^0.1.2",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-simplewatch": "0.0.1",
    "metalsmith-templates": "^0.5.2",
    "metalsmith-webpack": "^0.1.1",
    "mocha": "^1.21.3",
    "polyline": "0.0.3",
    "serve-static": "^1.3.2",
    "should": "^4.0.4",
    "webpack": "^1.3.1"
  },
  "jshintConfig": {
    "eqeqeq": true,
    "immed": true,
    "latedef": "nofunc",
    "newcap": true,
    "quotmark": "single",
    "trailing": true,
    "undef": true,
    "unused": true,
    "asi": true,
    "boss": true,
    "expr": true,
    "laxbreak": true,
    "laxcomma": true,
    "scripturl": true,
    "sub": true,
    "loopfunc": true,
    "browser": true,
    "node": true,
    "predef": [
      "d3",
      "describe",
      "it",
      "L"
    ]
  }
}
